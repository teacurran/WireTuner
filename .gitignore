# WireTuner .gitignore

# Miscellaneous
*.class
*.log
*.pyc
*.swp
.DS_Store
.atom/
.build/
.buildlog/
.history
.svn/
.swiftpm/
migrate_working_dir/
node_modules/

# Environment files
*.env
server/sync-api/.env

# Codemachine artifacts
.codemachine/memory/
.codemachine/logs/
.codemachine/prompts/
.codemachine/agents/
.codemachine/template.json

# IntelliJ related
*.iml
*.ipr
*.iws
.idea/

# The .vscode folder contains launch configuration and tasks you configure in
# VS Code which you may wish to be included in version control, so this line
# is commented out by default.
#.vscode/

# Flutter/Dart/Pub related
**/doc/api/
**/ios/Flutter/.last_build_id
.dart_tool/
.flutter-plugins
.flutter-plugins-dependencies
.packages
.pub-cache/
.pub/
/build/
packages/**/build/
server/**/build/

# Symbolication related
app.*.symbols

# Obfuscation related
app.*.map.json

# Android Studio will place build artifacts here
/android/app/debug
/android/app/profile
/android/app/release

# Generated files from freezed and build_runner
*.g.dart
*.freezed.dart

# CodeMachine internal files
.codemachine/memory/
.codemachine/logs/
.codemachine/prompts/
.codemachine/agents/
.codemachine/template.json
.codemachine/scripts/
# Note: .codemachine/artifacts/ is intentionally NOT ignored (task tracking)

# Test coverage reports
coverage/
.codemachine/logs
.codemachine/memory
.codemachine/prompts
.codemachine/agents

# Documentation media (large files - added per I3.T11)
# Exclude large screenshots/GIFs (>1 MB) from version control
# docs/assets/screenshots/*.png
# docs/assets/gifs/*.gif
# Uncomment above lines if binary assets exceed 1 MB
# Note: Keep placeholder READMEs tracked
# !docs/assets/screenshots/README.md
# !docs/assets/gifs/README.md
